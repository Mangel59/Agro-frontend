{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/docdash"
  }
}
