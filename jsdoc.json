{
  "plugins": ["node_modules/jsdoc-babel"],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(node_modules|docs)"
  },
  "babel": {
    "extensions": ["js", "jsx"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  }
}
